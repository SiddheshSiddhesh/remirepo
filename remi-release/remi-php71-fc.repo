[remi-php71]
name=Remi's RPM repository - PHP 7.1 - Fedora $releasever - $basearch
#baseurl=http://rpms.remirepo.net/fedora/$releasever/php71/$basearch/
mirrorlist=http://rpms.remirepo.net/fedora/$releasever/php71/mirror
# WARNING: If you enable this repository, you must also enable "remi"
enabled=0
gpgcheck=1
failovermethod=roundrobin
fastestmirror=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-remi
